package com.imaginegames.game.worlds.old;

public class LimitedWorld {
    private int width;
    private int height;
    private int[][] cells;

    private long seed;

    public LimitedWorld(long seed) {
        this.seed = seed;
    }

    public long getSeed() {
        return seed;
    }

    public LimitedWorld(int[][] cells) {
        this.cells = cells;
        this.height = cells.length;
        this.width = cells[0].length;
    }

    public int getWidth() {
        return width;
    }

    public int getHeight() {
        return height;
    }

    public int[][] getCells() {
        return cells;
    }
}
